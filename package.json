{
  "name": "red-cat-pictures",
  "version": "2.14.0",
  "description": "Nurture the essence of your product with our photography & videography services in kolkata",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "LGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/Algostract/red-cat-pictures.git"
  },
  "type": "module",
  "private": true,
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0 || ^10.2.0",
    "bun": "^1.2.9"
  },
  "packageManager": "bun@1.2.16",
  "scripts": {
    "prepare": "husky || true",
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "test:load": "k6 run tests/load.test.ts",
    "build": "nuxt build && cp -r static .output/static",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node --env-file=.env.prod .output/server/index.mjs",
    "clean": "nuxt cleanup",
    "docker:build": "docker build --build-arg TAG=dev -t red-cat-pictures:dev .",
    "docker:start": "docker run --detach --name red-cat-pictures-dev --mount type=bind,source=$(pwd)/static,target=/app/static --env-file .env.prod -p 4300:3000 red-cat-pictures:dev",
    "tunnel": "untun tunnel --port 3002 http://localhost:3002"
  },
  "dependencies": {
    "@notionhq/client": "^3.1.3",
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.14.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "0.11.5",
    "@nuxt/test-utils": "^3.19.1",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/seo": "^3.0.3",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@orama/orama": "^3.1.7",
    "@shba007/unai": "^0.3.5",
    "@types/k6": "^1.0.2",
    "@types/web-push": "^3.6.4",
    "@unhead/vue": "^2.0.10",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vue-email/components": "^0.0.21",
    "@vue-email/render": "^0.0.9",
    "@vueuse/core": "^13.4.0",
    "@vueuse/nuxt": "^13.4.0",
    "confbox": "^0.2.2",
    "date-fns": "^4.1.0",
    "eslint": "^9.29.0",
    "execa": "^9.6.0",
    "formidable": "^3.5.4",
    "hast-util-from-html": "^2.0.3",
    "hast-util-select": "^6.0.4",
    "image-meta": "^0.2.1",
    "ipx": "^3.0.3",
    "marked": "^15.0.12",
    "notion-to-md": "^3.1.9",
    "nuxt": "^3.17.5",
    "nuxt-nodemailer": "^1.1.2",
    "nuxt-splide": "^1.1.1",
    "puppeteer": "^24.10.2",
    "rehype-parse": "^9.0.1",
    "rehype-remark": "^10.0.1",
    "remark-stringify": "^11.0.0",
    "undio": "^0.2.0",
    "unified": "^11.0.5",
    "vue": "latest",
    "vue-router": "latest",
    "web-push": "^3.6.7",
    "whatsapp-web.js": "^1.30.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "playwright-core": "^1.53.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "typescript": "^5.8.3",
    "untun": "^0.1.3",
    "vitest": "^3.2.4"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "esbuild",
    "puppeteer",
    "sharp",
    "unrs-resolver"
  ]
}