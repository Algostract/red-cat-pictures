{
  "name": "red-cat-pictures",
  "version": "2.6.0",
  "description": "Nurture the essence of your product with our photography & videography services in kolkata",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "license": "LGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/Algostract/red-cat-pictures.git"
  },
  "type": "module",
  "private": true,
  "engines": {
    "node": "^20.15.0 || ^22.11.0",
    "pnpm": "^9.15.0 || ^10.2.0",
    "bun": "^1.2.9"
  },
  "packageManager": "bun@1.2.9",
  "scripts": {
    "prepare": "husky || true",
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev",
    "detect": "gitleaks git --verbose",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "test:load": "k6 run tests/load.test.ts",
    "build": "nuxt build && cp -r static .output/static",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node --env-file=.env.prod .output/server/index.mjs",
    "clean": "nuxt cleanup",
    "docker:build": "docker build --build-arg TAG=dev -t red-cat-pictures:dev .",
    "docker:start": "docker run --detach --name red-cat-pictures-dev --mount type=bind,source=$(pwd)/static,target=/app/static --env-file .env.prod -p 4102:3000 red-cat-pictures:dev",
    "tunnel": "untun tunnel --port 4102 http://localhost:4102"
  },
  "dependencies": {
    "@notionhq/client": "^2.3.0",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/fonts": "^0.11.1",
    "@nuxt/icon": "^1.12.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "0.11.5",
    "@nuxt/test-utils": "^3.17.2",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/seo": "3.0.2",
    "@nuxtjs/tailwindcss": "^6.13.2",
    "@types/k6": "^1.0.2",
    "@types/web-push": "^3.6.4",
    "@unhead/vue": "^2.0.8",
    "@vite-pwa/nuxt": "^1.0.0",
    "@vueuse/core": "^13.1.0",
    "@vueuse/nuxt": "^13.1.0",
    "confbox": "^0.2.2",
    "eslint": "^9.25.0",
    "execa": "^9.5.2",
    "marked": "^15.0.8",
    "notion-to-md": "^3.1.8",
    "nuxt": "^3.16.2",
    "nuxt-splide": "^1.1.1",
    "nuxt-time": "1.0.3",
    "undio": "^0.2.0",
    "vue": "latest",
    "vue-router": "latest",
    "web-push": "^3.6.7",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.3",
    "untun": "^0.1.3"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "unrs-resolver"
  ]
}